init:
    $ say_prolog_x = 100
    $ say_prolog_y = 100
    $ say_prolog_xsize = 1541
    $ say_prolog_textsize = 60

    python:
        def say_set(x = 0, y = 0, xsize = Null, size = Null):
            global say_prolog_x
            global say_prolog_y
            global say_prolog_xsize
            global say_prolog_textsize
            say_prolog_x = x
            say_prolog_y = y
            if xsize != Null:
                say_prolog_xsize = xsize
            if size != Null:
                say_prolog_textsize = size

screen say_prolog:
    text what:
        id "what"
        xpos say_prolog_x
        ypos say_prolog_y
        xmaximum say_prolog_xsize
        size say_prolog_textsize
        line_spacing 1
    
    
label Nekr:

    show menu_111 with dspr
    $ renpy.pause(0,5)
    play music menu_music fadein 3
    call screen menu11
    screen menu11:
        tag test
        modal True
        
        imagemap:
            idle "NekromantvSovenke/menu1.png"
            hover "NekromantvSovenke/menu2.png"
            alpha True
            
            hotspot(1255,230,610,120) action (Hide("idle")),Jump("prologue_nekr")
            hotspot(1050,440,835,145) action OpenURL("https://vk.com/nekromantvsovenke")
            hotspot(1400,665,460,205) action (Hide("idle")),Jump("returner")
      
        
        
    label returner:
    return
    
    
    
    
    
    
    
    
